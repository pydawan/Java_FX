package application;

import javafx.beans.property.DoubleProperty;

public class MeuNome {
	
	private DoubleProperty number;

	public DoubleProperty getNumber(Double ) {
		if(number != null)
		return number.get();
		return 0;
	}

	public void setNumber(DoubleProperty number) {
		this.number = number;
	}
	
	

}
