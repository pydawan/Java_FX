package application;
import java.sql.*;

public class SQLiteConnection {
	
	public static Connection conectar(){
		
		try {
			Class.forName("org.sqlite.JDBC");
			Connection con = DriverManager.getConnection("jdbc:sqlite:employeeDB.sqlite");
			return con;
		} catch (Exception e) {
			
			
			
		
		}
		
		
	}

}
