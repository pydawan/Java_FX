package application;

public class Model {
	
	public float calculadora(long num1, long num2, String operador){
		
		
		switch (operador) {
		case "+":
			num1 + num2;	
			break;
		case " - ":
			num1 - num2;
			break;
		case " * ":
			num1 * num2;
			

		default:
			break;
		}
		
		
		
		
		
		
		
		
		return 0;
		
	}

}
