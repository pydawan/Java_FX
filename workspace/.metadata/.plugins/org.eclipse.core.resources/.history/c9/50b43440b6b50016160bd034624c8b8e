package application;

import javafx.beans.property.*;

public class MyName {
	
	private DoubleProperty number;

	public double getNumber() {
		if(number != 0)
		return number.get();
		return 0;
	}

	public void setNumber(DoubleProperty number) {
		this.number = number;
	}
	
	public final DoubleProperty numberProperty(){
		if(number == null){
			number = new SimpleDoubleProperty(0);
		}
		return number;
	}

	
	

}
