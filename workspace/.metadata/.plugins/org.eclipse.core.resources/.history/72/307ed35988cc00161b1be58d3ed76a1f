package application;

public class Estudante {
	
	private final Integer id;
	private final String nome;
	private final Integer sobreNome;
	private final Integer idade;
	
	public Estudante(Integer id, String nome, Integer sobreNome, Integer idade) {
		super();
		this.id = id;
		this.nome = nome;
		this.sobreNome = sobreNome;
		this.idade = idade;
	}

	public Integer getId() {
		return id;
	}

	public String getNome() {
		return nome;
	}

	public Integer getSobreNome() {
		return sobreNome;
	}

	public Integer getIdade() {
		return idade;
	}
	
	

}
